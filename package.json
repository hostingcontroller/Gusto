{
  "name": "@hc/orchestrator",
  "private": true,
  "version": "0.0.11",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "npx tsx --env-file=./testing.env ./src/app.ts",
    "dev": "nodemon",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "4.21.0",
    "hostingcontroller-sdk": "1.0.8",
    "mysql2": "3.11.3"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.1",
    "typescript": "5.5.3"
  },
  "nodemon": {
    "watch": [
      "."
    ],
    "ext": "ts",
    "exec": "npm run start"
  }
}
